{
  "name": "@andyrmitchell/queue-sql",
  "version": "0.0.3",
  "description": "Queue function execution using sql to manage it. This is NOT a full job queue, it just enforces linearity in one apps execution path.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build_release": "npm run build_prepare && np",
    "build": "tsup",
    "pkglint": "./build/publint_pipeable.sh",
    "build_prepare": "npm run build && npm run pkglint",
    "prepublishOnly": "npm run build_prepare",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andymitchell/queue-sql.git"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=19.0.0"
  },
  "devDependencies": {
    "@andyrmitchell/drizzle-fast-bulk-test": "^0.1.5",
    "@andyrmitchell/file-io": "^0.10.12",
    "@electric-sql/pglite": "^0.2.14",
    "@libsql/client": "^0.14.0",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/uuid": "^9.0.8",
    "drizzle-kit": "^0.29.0",
    "postgres": "^3.4.5",
    "tsup": "^8.3.5",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7"
  },
  "dependencies": {
    "@andyrmitchell/drizzle-dialect-types": "^0.0.8",
    "@andyrmitchell/drizzle-robust-transaction": "^0.0.6",
    "@andyrmitchell/utils": "^0.11.4",
    "drizzle-orm": "^0.36.4",
    "lodash-es": "^4.17.21",
    "uuid": "^9.0.1"
  }
}
